{
  "name": "bender",
  "version": "1.0.0",
  "description": "Bender Robot",
  "main": "bin/hubot",
  "license": "MIT",
  "private": false,
  "keywords": [
    "hubot",
    "rocketchat",
    "chatbot",
    "chat"
  ],
  "engines": {
    "node": ">=10.x",
    "npm": ">=5.x"
  },
  "scripts": {
